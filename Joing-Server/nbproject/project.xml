<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://www.netbeans.org/ns/project/1">
    <type>org.netbeans.modules.j2ee.earproject</type>
    <configuration>
        <buildExtensions xmlns="http://www.netbeans.org/ns/ant-build-extender/1">
            <extension file="extendArchiveGF.xml" id="gfarchiveextend">
                <dependency dependsOn="-extend-archive" target="pre-dist"/>
            </extension>
        </buildExtensions>
        <data xmlns="http://www.netbeans.org/ns/j2ee-earproject/2">
            <name>Joing</name>
            <minimum-ant-version>1.6</minimum-ant-version>
            <web-module-libraries/>
            <web-module-additional-libraries>
                <library files="1">
                    <file>${reference.joing-ejb.dist-ear}</file>
                    <path-in-war>/</path-in-war>
                </library>
                <library files="1">
                    <file>${reference.joing-app-client.j2ee-module-car}</file>
                    <path-in-war>/</path-in-war>
                </library>
                <library files="1">
                    <file>${reference.joing-war.dist-ear}</file>
                    <path-in-war>/</path-in-war>
                </library>
            </web-module-additional-libraries>
        </data>
        <references xmlns="http://www.netbeans.org/ns/ant-project-references/1">
            <reference>
                <foreign-project>joing-app-client</foreign-project>
                <artifact-type>j2ee_ear_archive</artifact-type>
                <script>build.xml</script>
                <target>dist-ear</target>
                <clean-target>clean-ear</clean-target>
                <id>j2ee-module-car</id>
            </reference>
            <reference>
                <foreign-project>joing-ejb</foreign-project>
                <artifact-type>j2ee_ear_archive</artifact-type>
                <script>build.xml</script>
                <target>dist-ear</target>
                <clean-target>clean-ear</clean-target>
                <id>dist-ear</id>
            </reference>
            <reference>
                <foreign-project>joing-war</foreign-project>
                <artifact-type>j2ee_ear_archive</artifact-type>
                <script>build.xml</script>
                <target>dist-ear</target>
                <clean-target>clean-ear</clean-target>
                <id>dist-ear</id>
            </reference>
        </references>
    </configuration>
</project>
